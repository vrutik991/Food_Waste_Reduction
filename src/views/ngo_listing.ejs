<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>NGO Listing</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/styles.css">
</head>

<body>
    <div class="container">
        <ul>
            <div class="listings">
                <% ngo_listing.map((ngo)=> { %>
                    <div class="listing">
                        <a href="/food_details/showNgo/<%= ngo._id %>">
                            <h1>
                                <li>
                                    <%= ngo.name %>
                                </li>
                            </h1>
                        </a>
                        <li>City: <%= ngo.city %>
                        </li>
                        <li>State: <%= ngo.state %>
                        </li>
                        <li>Email: <%= ngo.email %>
                        </li>
                        <li>Contact: <%= ngo.contact %>
                        </li>
                        <li>Website: <%= ngo.website %>
                        </li>
                        <li>Registration Year: <%= ngo.registration_year %>
                        </li>
                        <br>
                        <br>
                        <br>
                    </div>
                    <%})%>
            </div>
        </ul>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js
    "></script>
</body>

</html>